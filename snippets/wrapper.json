{
  "wrapper el-card": {
    "prefix": "lwrapper_card",
    "body": ["<el-card class='box-card'>", "\t ${1}", "</el-card>"]
  },
  "wrapper el-form": {
    "prefix": "lwrapper_form",
    "body": [
      "<el-form :model='${1}' label-width='100px' style='width:600px'>",
      "\t ${2}",
      "</el-form>"
    ]
  },
  "wrapper el-form__validate": {
    "prefix": "lwrapper_form--validate",
    "body": [
      "<el-form :model='${1}' :rules='rules' ref='${1}Form' label-width='100px' style='width:600px'>",
      "\t ${2}",
      "\t <el-form-item>",
      "\t\t <el-button type='primary' @click=\"${2}('${1}Form')\">确定</el-button>",
      "\t </el-form-item>",
      "</el-form>"
    ]
  },
  "wrapper el-dialog": {
    "prefix": "lwrapper_dialog",
    "body": [
      "<el-dialog title='${1}' :visible.sync='dialogFormVisible'>",
      "\t ${2}",
      "</el-dialog>"
    ]
  },
  "wrapper el-table": {
    "prefix": "lwrapper_table",
    "body": [
      "<el-table :data='${1}' border style='width: 100%'>",
      "\t ",
      "</el-table>"
    ]
  },
  "wrapper table with opt": {
    "prefix": "lwrapper_table--opt",
    "body": [
      "<el-table :data='${1}' border style='width: 100%'>",
      "\t<el-table-column label='操作' align='center'>",
      "\t\t<template slot-scope='scope'>",
      "\t\t\t<el-button size='mini' type='primary' @click='${2}'>编辑</el-button>",
      "\t\t</template>",
      "\t</el-table-column>",
      "</el-table>"
    ]
  }
}
